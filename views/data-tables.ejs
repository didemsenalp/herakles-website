        <table id="myContactTable" class="table fullwidth mt-70">
          <thead>
            <tr>
              <th>Date</th>
              <th>Name</th>
              <th>E-mail</th>
              <th>Phone</th>
              <th>Message</th>
              <th>Reform Date</th>
