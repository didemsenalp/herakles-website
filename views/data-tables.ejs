  <style>
    /* .customized-table options */
    .customized-table div.dataTables_wrapper div.dataTables_length label {
      font-size: 16px;
      color: #4B4B4B;
    }

    .customized-table div.dataTables_wrapper {
      overflow: auto;
    }

    .customized-table table.dataTable {
      border: 1px solid #EAEAEA;
      margin-top: 40px !important;
      color: #9E9E9E;
      font-size: 13px;
      font-weight: 400 !important;
      text-transform: uppercase;
    }

    .customized-table div.dataTables_wrapper div.dataTables_length select {
      border: none;
      border-radius: 30px;
      background-color: #F4F4F4;
      outline: none !important;
      box-shadow: none !important;
      padding: 0 20px;
      width: 85px;
      height: 40px;
      margin: 0 5px;
    }

    .customized-table .table thead th {
      border: none;
      text-transform: uppercase;
    }

    .customized-table .table td,
    .customized-table .table th {
      border-top: 1px solid #EAEAEA;
      padding: 20px;
      text-transform: none;
      font-weight: 400;
    }

    .customized-table table.dataTable td {
      color: #4B4B4B;
    }

    .customized-table table.dataTable thead .sorting:before,
    .customized-table table.dataTable thead .sorting:after,
    .customized-table table.dataTable thead .sorting_asc:before,
    .customized-table table.dataTable thead .sorting_asc:after,
    .customized-table table.dataTable thead .sorting_desc:before,
    .customized-table table.dataTable thead .sorting_desc:after,
    .customized-table table.dataTable thead .sorting_asc_disabled:before,
    .customized-table table.dataTable thead .sorting_asc_disabled:after,
    .customized-table table.dataTable thead .sorting_desc_disabled:before,
    .customized-table table.dataTable thead .sorting_desc_disabled:after {
      opacity: .4;
      color: black;
      bottom: 1.5em;
    }

    .customized-table table.dataTable thead .sorting:before,
    .customized-table table.dataTable thead .sorting_asc:before,
    .customized-table table.dataTable thead .sorting_desc:before,
    .customized-table table.dataTable thead .sorting_asc_disabled:before,
    .customized-table table.dataTable thead .sorting_desc_disabled:before {
      right: 2em;
    }

    .customized-table table.dataTable thead .sorting:after,
    .customized-table table.dataTable thead .sorting_asc:after,
    .customized-table table.dataTable thead .sorting_desc:after,
    .customized-table table.dataTable thead .sorting_asc_disabled:after,
    .customized-table table.dataTable thead .sorting_desc_disabled:after {
      right: 1.5em;
    }

    .customized-table table.dataTable thead .sorting_asc:before,
    .customized-table table.dataTable thead .sorting_desc:after {
      opacity: 1;
    }

    .customized-table table.dataTable tr:hover {
      background-color: #fafafa;
    }

    .customized-table .table thead th:hover {
      color: #444;
    }

    /* Search input options */
    .customized-table div.dataTables_wrapper div.dataTables_filter input {
      border: none;
      background-color: #f4f4f4;
      margin-left: 15px;
      padding: 15px 20px;
      height: 45px;
      border-radius: 50px;
      width: 240px;
      box-shadow: none;
    }

    /* Margin for bottom text and pagination */
    .customized-table div.dataTables_wrapper div.dataTables_info,
    div.dataTables_wrapper div.dataTables_paginate {
      margin-top: 40px;
    }

    /* Pagination options */
    .customized-table .page-link {
      padding: 0.72rem 1.03rem;
      color: #717171;
      color: #777 !important;
      box-shadow: none !important;
      font-size: 12px;
    }

    .customized-table .page-item.active .page-link {
      background-color: #444;
      border-color: #444;
      color: white !important;
    }

    .customized-table .page-item:first-child .page-link,
    .page-item:last-child .page-link {
      border-radius: 0 !important;
    }

    /* Pagination position on small devices */
    @media only screen and (max-width:992px) {
      div.dataTables_wrapper div.dataTables_paginate ul.pagination {
        justify-content: center;
      }

      .customized-table table.dataTable thead .sorting:before,
      .customized-table table.dataTable thead .sorting_asc:before,
      .customized-table table.dataTable thead .sorting_desc:before,
      .customized-table table.dataTable thead .sorting_asc_disabled:before,
      .customized-table table.dataTable thead .sorting_desc_disabled:before {
        right: .9em;
      }

      .customized-table table.dataTable thead .sorting:after,
      .customized-table table.dataTable thead .sorting_asc:after,
      .customized-table table.dataTable thead .sorting_desc:after,
      .customized-table table.dataTable thead .sorting_asc_disabled:after,
      .customized-table table.dataTable thead .sorting_desc_disabled:after {
        right: 0.4em;
      }
    }
